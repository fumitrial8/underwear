.area_ranking_page
  %h1 #{params[:area].upcase} Ranking
  .area_ranking_page_all.row
    - @brands.each.with_index(1) do |brand, index|
      = link_to brand_path(brand) do
        .brand_container.col.s12.m12.l12
          %h1 No.#{index}
          = brand.name
          - begin
            = image_tag brand.image.to_s
          - rescue
%script{:src => "https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js", :type => "text/javascript"}    